## 人口と面積のデータについて考える
x <- subset(mydat,select=人口:面積)
## 地方ごとの人口と面積の平均値を求める
aggregate(x, by=list(地方=myreg$地方), FUN=mean)
## 以下のコードも同じ結果を返す
y <- data.frame(x,地方=myreg$地方)
aggregate( . ~ 地方, data=y, FUN = mean)
## 地方と人口の多寡でグループ分け
aggregate(x, by=list(地方=myreg$地方,
人口の多寡=(mydat$人口>median(mydat$人口))),
FUN=mean)
